        $('#language_select').change(function () {
            let lang_code = $(this).val()
            $.cookie('lang_code_cookie', lang_code, {path: '/'});
            console.log(lang_code)
            if (lang_code == 'en') {
                window.location.href = "/psirt/" + "FG-IR-25-756";
            } else {
                window.location.href = "/" + lang_code + "/psirt" + "/" + "FG-IR-25-756";
