	"document_notes": [
		{
			"text": "Security update for libvirt",
			"title": "Topic",
			"type": "Summary"
		},
		{
			"text": "\n      This update fixes the following security issues:\n\n- CVE-2015-0236: libvirt: access control bypass bsc#914693\n\n- bnc#905086: libvirt cannot properly determine cpu flags with qemu-kvm\n\n- Fixed allowing devices for containers.\n",
			"title": "Details",
			"type": "General"
		},
